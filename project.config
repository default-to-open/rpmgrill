[project]
	description = Static package analysis of RPM builds
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/sthaha (sthaha)
[access "refs/for/refs/*"]
	read = group user/sthaha (sthaha)
	push = group user/sthaha (sthaha)
	label-Code-Review = -2..+2 group user/sthaha (sthaha)
	label-Verified = -1..+1 group user/sthaha (sthaha)
	rebase = group user/sthaha (sthaha)
	removeReviewer = group user/sthaha (sthaha)
	submit = group user/sthaha (sthaha)
[access "refs/heads/*"]
	read = group user/sthaha (sthaha)
	create = group user/sthaha (sthaha)
	push = +force group user/sthaha (sthaha)
	pushMerge = group user/sthaha (sthaha)
[access "refs/tags/*"]
	push = +force group user/sthaha (sthaha)
	pushTag = group user/sthaha (sthaha)
	pushSignedTag = group user/sthaha (sthaha)
[submit]
	action = rebase if necessary
